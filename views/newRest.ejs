<script>
    // cursor follow
    const cursor = document.querySelector('.cursor-mini');
    document.addEventListener('mousemove', (e) => {
        cursor.style.left = e.pageX + 'px';
        cursor.style.top = e.pageY + 'px';
    })
</script>
<style>
    .newSection {
        min-height: 100vh;
        height: fit-content;
        width: 100%;
        display: flex;
        justify-content: center;
        align-items: center;
    }

    .newContainer {
        height: fit-content;
        width: 100%;
        max-width: 800px;
        margin: 40px;
        padding: 40px;
        border-radius: 20px;
        box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.2);
        display: flex;
        gap: 20px;
        transition: all 0.4s ease;
    }

    .newContainer:hover {
        box-shadow: 0 0 20px 0 rgba(0, 0, 0, 0.2);
        transition: all 0.4s ease;
    }

    .svgContainer {
        width: 100%;
        height: 100%;
        max-width: 600px;
        max-height: 600px;
        display: flex;
        flex-basis: 50%;
        margin: auto;
    }

    .subtitleNew {
        font-family: "Open Sans", sans-serif;
        font-size: 35px;
        font-weight: 500;
        max-width: 500px;
        text-align: center;
        margin: 20px auto;
    }

    .wrapper {
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        width: 100%;
        height: fit-content;
    }

    .form {
        width: 100%;
        max-width: 400px;
        height: fit-content;
        outline: none;
    }

    .input {
        border: none;
        border-bottom: 2px solid #57cc99;
        background: none;
        font-family: 'Open Sans', sans-serif;
        font-weight: 500;
        font-size: 16px;
        padding: 12px 16px;
        color: black;
        transition: 0.6s ease-in-out;
        position: relative;
        outline: none;
    }

    .label {
        font-family: 'Open Sans', sans-serif;
        font-weight: 400;
        font-size: 14px;
        color: black;
    }

    .form-group {
        display: flex;
        flex-direction: column;
        margin-bottom: 20px;
    }

    .button-container {
        display: flex;
        justify-content: center;
        align-items: center;
        gap: 20px;
        margin: 0 auto;
        width: fit-content;
        height: fit-content;
    }

    @media screen and (max-width: 900px) {
        .newContainer {
            flex-direction: column;
            gap: 20px;
        }

        .svgContainer {
            width: 100%;
            height: 100%;
            max-width: 300px;
            max-height: 300px;
            margin-top: 20px;
            display: flex;
        }
    }
</style>